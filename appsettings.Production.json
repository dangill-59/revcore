{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "SECURITY_NOTICE": "DO NOT add secrets to this file! All sensitive values MUST come from environment variables.",

  "_COMMENT_redis": "Set via environment: redis__Configuration, redis__InstanceName",
  "redis": {
    "Configuration": null,
    "InstanceName": "rev"
  },

  "_COMMENT_authDb": "Set via environment: authDb__connectionString, authDb__database",
  "authDb": {
    "connectionString": null,
    "database": "globalAuth"
  },

  "_COMMENT_rabbitMQ": "Set via environment: rabbitMQ__hostname, rabbitMQ__user, rabbitMQ__pass, etc.",
  "rabbitMQ": {
    "hostname": null,
    "port": 5672,
    "adminPort": 15672,
    "user": null,
    "pass": null,
    "exchange": "revNewMessages"
  },

  "_COMMENT_authentication": "CRITICAL: Set via environment: authentication__signingKey (min 18 chars)",
  "authentication": {
    "revConnect": null,
    "signingKey": null,
    "useAuth0": false
  },

  "_COMMENT_elasticsearch": "Set via environment: elasticsearch__nodes__0",
  "elasticsearch": {
    "nodes": []
  },

  "cacheFolder": "/var/cache/rev",

  "_COMMENT_mongoDb": "Set via environment: mongoDb__connectionString",
  "mongoDb": {
    "connectionString": null
  },

  "multisite": {
    "currentInstance": null,
    "custdomainTemplate": null,
    "enabled": true,
    "maxWorkspaces": 100,
    "storage": "s3"
  },

  "_COMMENT_s3Storage": "Set via environment: s3Storage__accesskey, s3Storage__secretkey, etc.",
  "s3Storage": {
    "region": "us-east-1",
    "bucket": null,
    "accesskey": null,
    "secretkey": null,
    "StorageRoot": "production",
    "customEndpoint": null,
    "endPointHttp": false,
    "downloadedStorageRoot": "/var/cache/rev/downloads",
    "temporaryFilesFolder": "/var/cache/rev/tmp"
  },

  "nativeInstallerUrl": null,
  "overrideUrlScheme": "https:443",
  "projectTemplates": null,

  "support": {
    "helpTopics": null,
    "siteadmins": []
  },

  "_COMMENT_textConfig": "Set via environment for production SMTP",
  "textConfig": {
    "smtpHost": null,
    "smtpPort": 587,
    "smtpServer": null
  },

  "useSwagger": false
}
